---
const { 
  title = "My Astro Site", 
  description = "Welcome to my Astro website" 
} = Astro.props;
---

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>{title}</title>

  <meta name="generator" content={Astro.generator} />

  <meta name="description" content={description} />
</head>
